} 
.TabView{
   color: navy;
   font-size:10pt;
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
} 
A.TabView:link{
   Text-Decoration: none;
   color: navy;
   font-size:10pt;
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
} 
A.TabView:visited{
   Text-Decoration: none;
   color: navy;
   font-size:10pt;
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
}
A.TabView:hover{
   Text-Decoration: none;
   color: #6495ED;
   font-size:10pt;
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
} 