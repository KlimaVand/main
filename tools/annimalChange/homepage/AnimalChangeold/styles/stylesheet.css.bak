/*General body*/
body{
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
	background-color: white;
}
/*General table*/
table{
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
   font-size: 12px;
   color: navy;
   vertical-align : top;
}
th{
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
   font-size: 12px;
   vertical-align : top;
}
td{
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
   font-size: 12px;
   vertical-align : top;
}
a:link{
	color: navy;
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
}
a:visited{
	color: navy;
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
}
a:hover{
   color: navy;
   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
   font-size: 12px;
 /*  background : Red;*/
}
a:active{
	color: navy;
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
}
